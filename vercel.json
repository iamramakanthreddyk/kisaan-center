{
  "version": 2,
  "public": true,
  "buildCommand": "cd kisaan-frontend && npm run build",
  "outputDirectory": "kisaan-frontend/dist",
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://kisaan-center-production.up.railway.app/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
