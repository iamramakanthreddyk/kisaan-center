{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://kisaan-center-production.up.railway.app/api/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/((?!api/).*)",
      "destination": "/404.html",
      "statusCode": 200
    }
  ]
}
